GET		/api			controllers.IndexController.get(input : String)
POST	/api			controllers.IndexController.post(request : Request)
PUT		/api			controllers.IndexController.put(request : Request)
DELETE	/api			controllers.IndexController.delete(input : String)

GET /swagger.json        controllers.Assets.at(path="/public", file="swagger.json")

GET /swagger-ui/*file        controllers.Assets.at(path="/public/swagger-ui", file)